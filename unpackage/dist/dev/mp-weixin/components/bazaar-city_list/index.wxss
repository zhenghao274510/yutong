.location_icon {
  width: 48rpx;
  height: 48rpx;
}
.citylist {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: relative;
}
.city-list-container {
  width: 100%;
  height: 100%;
  background-color: #ebebeb;
  font-size: 14px;
  color: #333;
}
.city-list-container .city-list-content {
  margin-right: 25px;
}
.city-list-container .city-title {
  padding-left: 15px;
  line-height: 30px;
}
.city-list-container .city-list {
  padding-right: 30px;
}
.city-list-container .city-title-letter {
  padding-left: 25px;
}
.city-list-container .city-list-block {
  background-color: #f5f5f5;
}
.city-list-container .city-list-block .city-item {
  height: 44px;
  line-height: 44px;
  margin-left: 15px;
  border-bottom: 1px solid #c8c7cc;
}
.city-list-container .city-list-block .city-item:last-child {
  border: 0;
}
.city-list-container .city-list-inline {
  background-color: #f5f5f5;
  padding-bottom: 8px;
  overflow: hidden;
}
.city-list-container .city-list-inline::after {
  content: "";
  clear: both;
}
.city-list-container .city-list-inline .location-city,
.city-list-container .city-list-inline .city-item {
  float: left;
  background: #fff;
  width: 29%;
  height: 33px;
  margin-top: 15px;
  margin-left: 4%;
  padding: 0 4px;
  border: 1px solid #e6e6e6;
  border-radius: 3px;
  line-height: 33px;
  text-align: center;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.city-list-container .city-list-inline .location-city {
  width: auto;
  min-width: 30%;
  padding: 0 20px;
}
.navrightcity {
  position: fixed;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  right: 0;
  width: 35px;
  z-index: 10;
  text-align: center;
  font-size: 12px;
}
.navrightcity .nav-item {
  height: 16px;
  height: 2.8vh;
}
.navrightcity .nav-letter {
  width: 15px;
  margin-left: 15px;
}
.fixtitle {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  overflow: hidden;
}
.fixtitle .city-title {
  padding-left: 15px;
  line-height: 30px;
  font-size: 14px;
  color: #333;
  background-color: #ebebeb;
}

