<view class="citylist"><scroll-view style="{{'height:'+(windowH)+';'}}" scroll-y="true" scroll-top="{{scrollTop}}" scroll-into-view="{{tabFixedCity}}" show-scrollbar="false" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view><view class="city-list-container"><view class="city-list-content" id="location_city"><view class="city-title">定位城市</view><view class="city-list city-list-inline  row"><view data-event-opts="{{[['tap',[['selectedCity',['$0'],['locationCity']]]]]}}" class="location-city" bindtap="__e">{{locationCity}}</view><view data-event-opts="{{[['tap',[['getUserLocation',['$event']]]]]}}" class="row cen" style="margin:30rpx 0 0 20rpx;" bindtap="__e"><image class="location_icon" src="/static/img/chongxindingwei.png"></image><view>点击重新定位</view></view></view></view><view class="city-list-content" id="hotcity"><view class="city-title">{{''+hotcity.title+''}}</view><view class="city-list city-list-inline"><block wx:for="{{hotcity.lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectedCity',['$0'],[[['hotcity.lists','',index]]]]]]]}}" class="city-item" bindtap="__e">{{''+item+''}}</view></block></view></view><view class="city-list-content" id="citytitle"><block wx:for="{{citylist}}" wx:for-item="city" wx:for-index="index" wx:key="index"><view class="city_title_wrap" id="{{'city'+index}}"><view class="city-title city-title-letter">{{''+city.title+''}}</view><view class="city-list city-list-block"><block wx:for="{{city.lists}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['selectedCity',['$0'],[[['citylist','',index],['lists','',k]]]]]]]}}" class="city-item" bindtap="__e">{{''+v+''}}</view></block></view></view></block></view></view></view></scroll-view><view class="fixtitle" style="{{'transform:'+(fixedStyle)+';'}}"><view class="city-title">{{fixedTitle}}</view></view><view class="navrightcity"><view data-event-opts="{{[['tap',[['scroll_to_city',[0]]]]]}}" class="nav-item nav-letter" bindtap="__e">定</view><view data-event-opts="{{[['tap',[['scroll_to_city',[1]]]]]}}" class="nav-item nav-letter" bindtap="__e">热</view><block wx:for="{{citylist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="nav-item nav-letter" id="{{'nav'+index}}" data-event-opts="{{[['tap',[['scroll_to_city',[index+2]]]]]}}" bindtap="__e">{{''+item.title+''}}</view></block></view></view>